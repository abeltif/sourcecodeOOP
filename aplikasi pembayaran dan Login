    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
String USERNAME = " ABEL ";
String PASSWORD = "202013004";
if(USERNAME.equalsIgnoreCase (jTextField1.getText()) && PASSWORD.equalsIgnoreCase(jTextField2.getText())) {
    
    this.setVisible(false);
    new PembayaranBelanja().setVisible(true);
    }else{
        
            //javax.swing.JOptionPane.showMessageDialog(null= "MAAF USERNAME AND PASSWORD ANDA SALAH");
            jTextField1.setText(""); 
            jTextField2.setText("");
            jTextField1.requestFocus();
    
    }                                        

